MEMORY
{
    VECTORS ( rx )    : ORIGIN = 0x08000000, LENGTH = 8
    FLASH ( rx )      : ORIGIN = 0x08000008, LENGTH = 64K - 8
    RAM ( rxw )       : ORIGIN = 0x20000000, LENGTH = 20K
}

_estack = ORIGIN(RAM) + LENGTH(RAM);

SECTIONS
{
    .isr_vector :
    {
        KEEP(*(.isr_vector))
    } > VECTORS

    .text :
    {
        *(.text)
        *(.text*)
    } > FLASH
}
